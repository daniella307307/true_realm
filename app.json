{
  "expo": {
    "name": "Africollect",
    "slug": "africollect",
    "runtimeVersion": "1.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-header.png",
    "scheme": "africollect",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#ffffff",
    "splash": {
      "image": "./assets/images/logo-header.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/formio/**/*",
      "assets/**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "supportsTablet": true,
      "icon": "./assets/images/logo-header.png",
      "bundleIdentifier": "com.afriqollect.mobile",
      "splash": {
        "image": "./assets/images/logo-header.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-header.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.afriqollect.mobile",
      "splash": {
        "image": "./assets/images/logo-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-header.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "a7a207c0-4b11-47f7-a8e8-09c5fa56e479"
      },
      "router": {}
    },
    "owner": "mitcheelariette"
  }
}
