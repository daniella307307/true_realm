{
  "expo": {
    "name": "Afriqollect",
    "slug": "africollect",
    "runtimeVersion": "1.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-header.png",
    "scheme": "africollect",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#ffffff",
    "splash": {
      "image": "./assets/images/logo-header.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/formio/**/*",
      "assets/**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos for profile pictures.",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera to take profile pictures.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos to your library."
      },
      "supportsTablet": true,
      "icon": "./assets/images/logo-header.png",
      "bundleIdentifier": "com.afriqollect.mobile",
      "splash": {
        "image": "./assets/images/logo-header.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-header.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.afriqollect.mobile",
      "splash": {
        "image": "./assets/images/logo-header.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-header.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos to your library."
        }
      ],
      "expo-router",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5318bd0c-a11f-44c5-add1-7b55ff2c7dba"
      }
    },
    "owner": "ganza_daniella"
  }
}